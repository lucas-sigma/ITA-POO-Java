package imc;

public class Principal {
	

	public static void main(String[] args) {
		Paciente p1 = new Paciente(45.54, 1.76);
		Paciente p2 = new Paciente(54.6, 1.81);
		Paciente p3 = new Paciente(87.8, 1.55);
		
		p1.calcularIMC();
		p1.diagnostico();
		
		p2.calcularIMC();
		p2.diagnostico();

		p3.calcularIMC();
		p3.diagnostico();

	}

}
