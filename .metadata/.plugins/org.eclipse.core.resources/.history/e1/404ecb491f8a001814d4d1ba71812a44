package imc;

public class Paciente {
	double peso, altura, imc;
	
	public Paciente(double peso, double altura) {
		this.peso = peso;
		this.altura = altura;
	}
	
	public double getPeso() {
		return peso;
	}
	
	public double getAltura() {
		return altura;
	}
	
	public double calcularIMC() {
		this.imc = (getPeso() / (getAltura() * getAltura()));
	}
}
